<!doctype html>
<html>
  <head>
    <title>Welcome to <%=shopData.shopName%></title>
    <link rel="stylesheet"  type="text/css" href="/main.css" />
  </head>
  <body>
    <h1><%=shopData.shopName%> Shop</h1>
    <h2>Here are the users on Betty's Books:</h2>
    <ul>
      <% availableUsers.forEach(function(user) { %>
        <li>Name: <%= user.firstName %> <%=user.lastName%>, Username: <%=user.username%>, Email: <%=user.email%></li>
      <% }) %>
    </ul>
    </body>
</html>
